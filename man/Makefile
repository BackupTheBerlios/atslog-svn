all:	atslogd atslogmaster

atslogd:
	@export LANG=en_US;txt2man -s 8 -t "atslogd" en/atslogd.8.txt > en/atslogd.8
	@export LANG=en_US;txt2man -s 8 -t "atslogd" ru.KOI8-R/atslogd.8.txt > ru.KOI8-R/atslogd.8
atslogmaster:
	@export LANG=en_US;txt2man -s 8 -t "atslogmaster" en/atslogmaster.8.txt > en/atslogmaster.8
	@export LANG=en_US;txt2man -s 8 -t "atslogmaster" ru.KOI8-R/atslogmaster.8.txt > ru.KOI8-R/atslogmaster.8
